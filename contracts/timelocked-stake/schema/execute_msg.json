{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "description": "Starts the undelegation process",
      "type": "object",
      "required": [
        "undelegate"
      ],
      "properties": {
        "undelegate": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Redelegate stake from one validator to another",
      "type": "object",
      "required": [
        "redelegate"
      ],
      "properties": {
        "redelegate": {
          "type": "object",
          "required": [
            "dst_validator"
          ],
          "properties": {
            "dst_validator": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Claim rewards from validator",
      "type": "object",
      "required": [
        "claim"
      ],
      "properties": {
        "claim": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Withdraw rewards and delegate to the validator",
      "type": "object",
      "required": [
        "reinvest"
      ],
      "properties": {
        "reinvest": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Private message to delegate as part of `Reinvest {}`",
      "type": "object",
      "required": [
        "__delegate"
      ],
      "properties": {
        "__delegate": {
          "type": "object"
        }
      },
      "additionalProperties": false
    }
  ]
}
